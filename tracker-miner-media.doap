  <homepage rdf:resource="https://wiki.gnome.org/Projects/Tracker" />
  <developer-forum rdf:resource="https://discourse.gnome.org/tag/tracker" />
  <download-page rdf:resource="https://download.gnome.org/sources/tracker-miner-media/" />
  <bug-database rdf:resource="https://gitlab.gnome.org/GNOME/tracker-miner-media/issues/" />

  <programming-language>C</programming-language>

  <repository>
    <GitRepository>
      <location rdf:resource="https://gitlab.gnome.org/GNOME/tracker-miner-media.git" />
      <browse rdf:resource="https://gitlab.gnome.org/GNOME/tracker-miner-media" />
    </GitRepository>
  </repository>

  <maintainer>
    <foaf:Person>
      <foaf:name>Carlos Garnacho</foaf:name>
      <foaf:mbox rdf:resource="mailto:carlosg@gnome.org" />
